{
  "expo": {
    "name": "Whitecode",
    "slug": "whitecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Whitecode_icon.png",
    "splash": {
      "image": "./assets/Whitecode_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Whitecode_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.whitecode.whitecode"
    },
    "web": {
      "favicon": "./assets/Whitecode_icon.png"
    },
    "description": "Show casing my first native app",
    "githubUrl": "https://github.com/Ibnyahyah/whitecode_nativeApp"
  }
}
